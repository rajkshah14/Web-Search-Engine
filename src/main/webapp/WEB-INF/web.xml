<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>mysearch</servlet-name>
		<servlet-class>project08.servlet.SubmitCrawlerTask</servlet-class>
	</servlet>
		
	<servlet>
		<servlet-name>querysuggestion</servlet-name>
		<servlet-class>project08.servlet.QuerySuggestion</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>mysearch</servlet-name>
		<url-pattern>/project08/search</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>querysuggestion</servlet-name>
		<url-pattern>/project08/querysuggestion</url-pattern>
	</servlet-mapping>
</web-app>
